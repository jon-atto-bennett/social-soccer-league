{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "engines": {
    "node": "7.8.0"
  },
  "description": "A webpack React starter",
  "repository": "https://github.com/jon-atto-bennett/social-soccer-league.git",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "webpack -p",
    "knex": "knex",
    "test": "tape -r babel-register 'test/**/*.test.js' | tap-diff",
    "watch": "nodemon -e js,jsx -x npm test",
    "dev": "nodemon server server/index & webpack --watch",
    "debug": "node --inspect --debug-brk server/index",
    "postinstall": "knex migrate:lastest && npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "airbnb",
      "stage-0"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "jsx-loader": "^0.13.2",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sqlite3": "^3.1.8",
    "standard": "^9.0.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cors": "^2.7.1",
    "enzyme": "^2.8.2",
    "express": "^4.14.0",
    "jsdom": "^10.1.0",
    "knex": "^0.12.8",
    "nodemon": "^1.11.0",
    "sinon": "^2.2.0",
    "superagent": "^2.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3",
    "pg": "^6.1.5"
  }
}
